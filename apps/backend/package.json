{
  "name": "@asksync/backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "convex dev",
    "predev": "convex dev --until-success",
    "build": "convex deploy --dry-run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf _generated dist",
    "test": "vitest",
    "test:once": "vitest run",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "vitest run --coverage --coverage.reporter=text"
  },
  "dependencies": {
    "@asksync/shared": "workspace:*",
    "@convex-dev/migrations": "^0.3.0",
    "convex": "^1.31.0"
  },
  "devDependencies": {
    "@asksync/eslint-config": "workspace:*",
    "@asksync/typescript-config": "workspace:*",
    "@edge-runtime/vm": "^5.0.0",
    "@types/node": "^24.3.3",
    "convex-test": "^0.0.41",
    "eslint": "^9.0.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.15"
  }
}
